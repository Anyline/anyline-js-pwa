{
  "name": "anyline-js",
  "version": "1.0.2",
  "description": "anyline sdk in the browser",
  "main": "anyline.js",
  "scripts": {
    "demo": "npx http-server -S -C cert/cert.pem -K cert/key.pem -P 3000",
    "deploy": "gh-pages -d .",
    "start": "parcel serve --no-source-maps demo/index.html",
    "build": "rm -rf dist && parcel build --no-source-maps --public-url . demo/index.html",
    "deploy2": "yarn build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anyline/anyline-ocr-anylinejs-module.git"
  },
  "keywords": [
    "ocr",
    "anyline",
    "wasm",
    "vision",
    "scanner",
    "reader"
  ],
  "author": "Matthias Klan <matthias.klan@anyline.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/Anyline/anyline-ocr-anylinejs-module/issues"
  },
  "homepage": "https://github.com/Anyline/anyline-ocr-anylinejs-module#readme",
  "dependencies": {
    "gh-pages": "^3.1.0",
    "parcel": "^1.12.4",
    "parcel-plugin-sw-cache": "^0.3.1"
  },
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9"
  }
}
